      <section class="featured spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title">
                        <h2>Featured Product</h2>
                    </div>
                    <div class="featured__controls">
                        <ul>
                            <li class="active" data-filter="*">All</li>
                            @foreach(var items_Category in Model.Products_Category)
                            {

                                <li data-filter=".@items_Category.CategoryName">@items_Category.CategoryName</li>
                            }
                         
                        </ul>
                    </div>
                </div>
            </div>
            <div class="row featured__filter">
            @foreach(var item in Model.Products)
            {
                string ImgPath = "~/Images/ProductImages/" + item.ImagePath;
                    

                <div class="col-lg-3 col-md-4 col-sm-6 mix @item.Category">
                    <div class="featured__item">
                        <div class="featured__item__pic set-bg" data-setbg="@Url.Content(ImgPath)">
